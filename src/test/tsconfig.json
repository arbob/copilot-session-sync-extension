{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2022",
    "outDir": "../out/test",
    "lib": ["ES2022"],
    "sourceMap": true,
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "paths": {
      "../*": ["../src/*"]
    }
  },
  "include": ["./**/*.ts", "../src/**/*.ts"]
}
